<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.maodd.dao.UserMapper">

    <select id="getUserList" resultType="myUser">
        select * from mybatis.user
      </select>

    <select id="getUserListByPage" parameterType="map" resultType="myUser">
        select * from mybatis.user limit #{pageIndex},#{pageSize}
      </select>
    <select id="getUserListByRowBounds" resultType="myUser">
        select * from mybatis.user
      </select>

</mapper>

